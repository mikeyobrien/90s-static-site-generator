<%# Breadcrumb navigation %>
<% if (page.breadcrumb && page.breadcrumb.length > 0) { %>
<table width="100%" border="0" cellpadding="2" cellspacing="0">
  <tr>
    <td>
      <font size="2" face="Arial, sans-serif" color="#666666">
        <a href="/">Home</a>
        <% page.breadcrumb.forEach(function(crumb) { %>
          &gt; <a href="<%= crumb.url %>"><%= crumb.title %></a>
        <% }); %>
        &gt; <b><%= page.title %></b>
      </font>
    </td>
  </tr>
</table>
<% } %>